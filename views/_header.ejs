<header>
    <div id="name">
        <a href="/"><img src="/images/dailyhabitlogo.png"></a>
    </div>
    <div class="action-button">
    <ul>
        <% if (locals.user){ %>
            <li> Welcome <%= user.name %></li>
            <li id="sign-out-button">
                <a href="/users/sign-out">Sign Out</a>
            </li>
        <% }else{ %>
